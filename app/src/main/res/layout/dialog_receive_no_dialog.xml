<?xml version="1.0" encoding="utf-8"?>
<layout>
    <data>

    </data>
    <FrameLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        tools:context=".ui.receive.ReceiveNoDiagloFragment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/cl_receive_reject_dialog_top"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toTopOf="@id/cl_receive_reject_dialog_bottom">

                <TextView
                    android:id="@+id/tv_receive_dialog_reject"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/spoqa_han_sans_neo_bold"
                    android:textSize="18sp"
                    android:textColor="@color/gray06"
                    android:letterSpacing="-0.01"
                    android:text="약속 거절"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    android:layout_marginStart="24dp"
                    android:layout_marginTop="25dp"/>

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/cl_receive_reject_dialog_bottom"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                app:layout_constraintDimensionRatio="280:126"
                android:background="@color/white"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@id/cl_receive_reject_dialog_top">

                <TextView
                    android:id="@+id/tv_receive_reject"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/spoqa_han_sans_neo_bold"
                    android:textStyle="bold"
                    android:textSize="16sp"
                    android:textColor="@color/gray05"
                    android:letterSpacing="-0.01"
                    android:text="약속을 거절하시겠어요?"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    android:layout_marginStart="24dp"
                    android:layout_marginTop="25dp"
                    android:layout_marginEnd="97dp"/>

                <TextView
                    android:id="@+id/btn_receive_dia_reject_no"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/spoqa_han_sans_neo_medium"
                    android:textStyle="normal"
                    android:textSize="16sp"
                    android:textColor="@color/gray06"
                    android:letterSpacing="-0.01"
                    android:gravity="center_horizontal"
                    android:layout_marginBottom="6dp"
                    android:paddingBottom="14dp"
                    android:paddingEnd="10dp"
                    android:paddingStart="14dp"
                    android:paddingTop="14dp"
                    android:layout_marginTop="24dp"
                    android:text="아니요"
                    app:layout_constraintTop_toBottomOf="@id/tv_receive_reject"
                    app:layout_constraintEnd_toStartOf="@id/btn_receive_dia_reject_yes"
                    app:layout_constraintBottom_toBottomOf="parent"/>

                <TextView
                    android:id="@+id/btn_receive_dia_reject_yes"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/spoqa_han_sans_neo_medium"
                    android:textStyle="normal"
                    android:textSize="16sp"
                    android:textColor="@color/pink01"
                    android:letterSpacing="-0.01"
                    android:gravity="center_horizontal"
                    android:paddingBottom="14dp"
                    android:paddingEnd="25dp"
                    android:paddingStart="20dp"
                    android:paddingTop="14dp"
                    android:layout_marginBottom="6dp"
                    android:layout_marginStart="4dp"
                    android:text="예"
                    android:layout_marginTop="24dp"
                    app:layout_constraintTop_toBottomOf="@id/tv_receive_reject"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"/>



            </androidx.constraintlayout.widget.ConstraintLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>

    </FrameLayout>
</layout>
