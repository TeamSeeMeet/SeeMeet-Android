<?xml version="1.0" encoding="utf-8"?>
<layout>
   <data>

   </data>
    <FrameLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        tools:context=".ui.receive.ReceiveYesDialogFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/cl_recei_dialog_top"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toTopOf="parent"
                android:background="@color/gray01">

                <TextView
                    android:id="@+id/tv_reci_dialog_wish"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/spoqa_han_sans_neo_bold"
                    android:textSize="18sp"
                    android:textColor="@color/gray06"
                    android:letterSpacing="-0.01"
                    android:text="담은 목록"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    android:layout_marginStart="24dp"
                    android:layout_marginTop="25dp"/>

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/cl_recei_dialog_times"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="24dp"
                    android:layout_marginStart="24dp"
                    android:layout_marginEnd="24dp"
                    android:layout_marginBottom="21dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/tv_reci_dialog_wish"
                    app:layout_constraintBottom_toBottomOf="parent">


                    <TextView
                        android:id="@+id/tv_recei_date_wish1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/din_pro_bold"
                        android:textSize="18sp"
                        android:textColor="#000000"
                        android:letterSpacing="-0.01"
                        tools:text="2021.12.23"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"/>
                    <TextView
                        android:id="@+id/tv_recei_time_wish1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/din_pro_regular"
                        android:textSize="14sp"
                        android:textColor="#000000"
                        android:letterSpacing="-0.01"
                        android:gravity="end"
                        android:layout_marginStart="9dp"
                        app:layout_constraintStart_toEndOf="@id/tv_recei_date_wish1"
                        tools:text="오전 11:00 ~  오후 4:00 "
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:layout_marginTop="3dp"/>

                    <TextView
                        android:id="@+id/tv_recei_date_wish2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/din_pro_bold"
                        android:textSize="18sp"
                        android:textColor="#000000"
                        android:letterSpacing="-0.01"
                        tools:text="2021.12.23"
                        android:layout_marginTop="24dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/tv_recei_date_wish1"/>
                    <TextView
                        android:id="@+id/tv_recei_time_wish2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/din_pro_regular"
                        android:textSize="14sp"
                        android:textColor="#000000"
                        android:letterSpacing="-0.01"
                        android:gravity="end"
                        tools:text="오전 11:00 ~  오후 4:00 "
                        android:layout_marginTop="27dp"
                        android:layout_marginStart="9dp"
                        app:layout_constraintStart_toEndOf="@id/tv_recei_date_wish2"
                        app:layout_constraintTop_toBottomOf="@id/tv_recei_date_wish1"
                        app:layout_constraintEnd_toEndOf="parent" />

                    <TextView
                        android:id="@+id/tv_recei_date_wish3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/din_pro_bold"
                        android:textSize="18sp"
                        android:textColor="#000000"
                        android:letterSpacing="-0.01"
                        tools:text="2021.12.23"
                        android:layout_marginTop="24dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/tv_recei_date_wish2"/>
                    <TextView
                        android:id="@+id/tv_recei_time_wish3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/din_pro_regular"
                        android:textSize="14sp"
                        android:textColor="#000000"
                        android:letterSpacing="-0.01"
                        android:gravity="end"
                        tools:text="오전 11:00 ~  오후 4:00 "
                        android:layout_marginTop="27dp"
                        android:layout_marginStart="9dp"
                        app:layout_constraintStart_toEndOf="@id/tv_recei_date_wish3"
                        app:layout_constraintTop_toBottomOf="@id/tv_recei_date_wish2"
                        app:layout_constraintEnd_toEndOf="parent" />

                    <TextView
                        android:id="@+id/tv_recei_date_wish4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/din_pro_bold"
                        android:textSize="18sp"
                        android:textColor="#000000"
                        android:letterSpacing="-0.01"
                        tools:text="2021.12.23"
                        android:layout_marginTop="24dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/tv_recei_date_wish3"/>
                    <TextView
                        android:id="@+id/tv_recei_time_wish4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/din_pro_regular"
                        android:textSize="14sp"
                        android:textColor="#000000"
                        android:letterSpacing="-0.01"
                        android:gravity="end"
                        tools:text="오전 11:00 ~  오후 4:00 "
                        android:layout_marginTop="27dp"
                        android:layout_marginStart="9dp"
                        app:layout_constraintStart_toEndOf="@id/tv_recei_date_wish4"
                        app:layout_constraintTop_toBottomOf="@id/tv_recei_date_wish3"
                        app:layout_constraintEnd_toEndOf="parent" />


                </androidx.constraintlayout.widget.ConstraintLayout>

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:src="@color/gray02"
                    app:layout_constraintBottom_toBottomOf="parent"/>
            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/cl_recei_dialog_bottom"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:background="@color/white"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@id/cl_recei_dialog_top">

                <TextView
                    android:id="@+id/tv_recei_send"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/spoqa_han_sans_neo_medium"
                    android:textStyle="normal"
                    android:textSize="16sp"
                    android:textColor="@color/gray05"
                    android:letterSpacing="-0.01"
                    android:text="이렇게 보낼까요?"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    android:layout_marginStart="24dp"
                    android:layout_marginTop="17dp"/>

                <TextView
                    android:id="@+id/btn_recei_dia_cancel"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/spoqa_han_sans_neo_medium"
                    android:textStyle="normal"
                    android:textSize="16sp"
                    android:textColor="@color/gray06"
                    android:letterSpacing="-0.01"
                    android:gravity="center_horizontal"
                    android:layout_marginBottom="6dp"
                    android:paddingBottom="14dp"
                    android:paddingEnd="10dp"
                    android:paddingStart="14dp"
                    android:paddingTop="14dp"
                    android:layout_marginTop="24dp"
                    android:text="취소"
                    app:layout_constraintTop_toBottomOf="@id/tv_recei_send"
                    app:layout_constraintEnd_toStartOf="@id/btn_recei_dia_send"
                    app:layout_constraintBottom_toBottomOf="parent"/>

                <TextView
                    android:id="@+id/btn_recei_dia_send"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/spoqa_han_sans_neo_medium"
                    android:textStyle="normal"
                    android:textSize="16sp"
                    android:textColor="@color/pink01"
                    android:letterSpacing="-0.01"
                    android:gravity="center_horizontal"
                    android:paddingBottom="14dp"
                    android:paddingEnd="9dp"
                    android:paddingStart="11dp"
                    android:paddingTop="14dp"
                    android:layout_marginBottom="6dp"
                    android:text="보내기"
                    android:layout_marginTop="24dp"
                    android:layout_marginEnd="6dp"
                    app:layout_constraintTop_toBottomOf="@id/tv_recei_send"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"/>



            </androidx.constraintlayout.widget.ConstraintLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>

    </FrameLayout>
</layout>
