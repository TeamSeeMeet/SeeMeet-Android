<?xml version="1.0" encoding="utf-8"?>
<layout>
<data>

</data>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ui.main.home.DialogHomeNoLoginFragment"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cl_home_nologin_dialog_top"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toTopOf="@id/cl_home_nologin_dialog_bottom">

            <TextView
                android:id="@+id/tv_home_nologin_dialog"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/spoqa_han_sans_neo_bold"
                android:textSize="18sp"
                android:textColor="@color/gray06"
                android:letterSpacing="-0.01"
                android:text="로그인"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginStart="24dp"
                android:layout_marginTop="25dp"/>

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cl_home_nologin_dialog_bottom"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintDimensionRatio="280:126"
            android:background="@color/white"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/cl_home_nologin_dialog_top">

            <TextView
                android:id="@+id/tv_home_nologin_msg"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/spoqa_han_sans_neo_medium"
                android:textStyle="normal"
                android:textSize="16sp"
                android:textColor="@color/gray05"
                android:letterSpacing="-0.01"
                android:text="로그인이 필요한 서비스예요.\n로그인하러 갈까요?"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginStart="24dp"
                android:layout_marginTop="14dp"
                android:layout_marginEnd="51dp"/>

            <TextView
                android:id="@+id/btn_home_nologin_dialog_cancel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/spoqa_han_sans_neo_medium"
                android:textStyle="normal"
                android:textSize="16sp"
                android:textColor="@color/gray06"
                android:letterSpacing="-0.01"
                android:gravity="center_horizontal"
                android:layout_marginBottom="6dp"
                android:paddingBottom="14dp"
                android:paddingEnd="10dp"
                android:paddingStart="14dp"
                android:paddingTop="14dp"
                android:layout_marginTop="18dp"
                android:text="아니요"
                app:layout_constraintTop_toBottomOf="@id/tv_receive_reject"
                app:layout_constraintEnd_toStartOf="@id/btn_home_nologin_dialog_login"
                app:layout_constraintBottom_toBottomOf="parent"/>

            <TextView
                android:id="@+id/btn_home_nologin_dialog_login"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/spoqa_han_sans_neo_medium"
                android:textStyle="normal"
                android:textSize="16sp"
                android:textColor="@color/pink01"
                android:letterSpacing="-0.01"
                android:gravity="center_horizontal"
                android:paddingBottom="14dp"
                android:paddingEnd="25dp"
                android:paddingStart="20dp"
                android:paddingTop="14dp"
                android:layout_marginBottom="6dp"
                android:layout_marginStart="4dp"
                android:text="로그인"
                android:layout_marginTop="18dp"
                app:layout_constraintTop_toBottomOf="@id/tv_receive_reject"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"/>


        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>
</layout>
